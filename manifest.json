{
  "manifest_version": 3,
  "name": "Tab Count Logger",
  "version": "1.0",
  "description": "Logs browser tab count periodically.",
  "permissions": [
    "tabs",
    "storage",
    "alarms"
  ],
  "host_permissions": [
    "http://localhost:8086/*",
    "https://localhost:8086/*"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "options_page":"options.html"
}
